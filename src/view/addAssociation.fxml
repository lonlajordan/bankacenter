                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets left="20.0" top="40.0" />
               </padding>
            </HBox>
            <Separator layoutX="10.0" layoutY="214.0" prefWidth="200.0">
